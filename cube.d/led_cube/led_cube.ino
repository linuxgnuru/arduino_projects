#define CUBESIZE 4
#define PLANESIZE CUBESIZE*CUBESIZE
#define PLANETIME 3333 // time each plane is displayed in us -> 100 Hz refresh
#define TIMECONST 20 // multiplies DisplayTime to get ms - why not =100?

const byte LATCH = 10;

const int PlanePin[] = {
  4, 5, 6, 7
};

void setup()
{
  SPI.begin();
  for (int i = 0; i < CUBESIZE; i++)
    pinMode(PlanePin[i], OUTPUT);
  // put your setup code here, to run once:
  Serial.begin(9600);
}

byte data1 = 0b00000000;
byte data2 = 0b00000000;

void loop()
{
  byte PatternBuf[PLANESIZE]; // saves current pattern from PatternTable
  int PatternIdx;
  byte DisplayTime; // time*100ms to display pattern
  unsigned long EndTime;
  int plane; // loop counter for cube refresh
  int patbufidx = 0; // indexes which byte from pattern buffer
  int ledrow; // counts LEDs in refresh loop
  int ledcol; // counts LEDs in refresh loop
  int ledpin; // counts LEDs in refresh loop
  // put your main code here, to run repeatedly:
  PatternIdx = 0;
  do
  {
    // read pattern from PROGMEM and save in array
    memcpy_P(PatternBuf, PatternTable + PatternIdx, PLANESIZE);
    PatternIdx += PLANESIZE; // 4 * 4 = 16
    // read DisplayTime from PROGMEM and increment index
    DisplayTime = pgm_read_byte_near(PatternTable + PatternIdx++);
    // compute EndTime from current time (ms) and DisplayTime
    EndTime = millis() + ((unsigned long) DisplayTime) * TIMECONST;
    // loop while DisplayTime>0 and current time < EndTime
    while (millis() < EndTime)
    {
      patbufidx = 0; // reset index counter to beginning of buffer
      // loop over planes
      Serial.print("[");
      for (plane = 0; plane < CUBESIZE; plane++) // CUBESIZE = 4
      {
        // turn previous plane off
        digitalWrite(PlanePin[(plane == 0 ? CUBESIZE : plane) - 1], HIGH);
        // load current plane pattern data into ports
        //        ledpin = 0;
        //                        4
//          for (ledrow = 0; ledrow < CUBESIZE; ledrow++)
//        {
            data1 = PatternBuf[patbufidx] + PatternBuf[patbufidx + 1];
            data2 = PatternBuf[patbufidx + 2] + PatternBuf[patbufidx + 3];
          digitalWrite(LATCH, LOW);
          SPI.transfer(data2);
          SPI.transfer(data1);
          digitalWrite(LATCH, HIGH);
          Serial.print(data1);
          Serial.print("] [");
          Serial.print(data2);
//          Serial.print(PatternBuf[patbufidx]);// & (1 << ledcol);
          /*
          for (ledcol = 0; ledcol < CUBESIZE; ledcol++)
          {
          //            Serial.print("[");
            PatternBuf[patbufidx] & (1 << ledcol);
          //            PatternBuf[patbufidx] & (1 << ledcol)
            Serial.print(PatternBuf[patbufidx] & (1 << ledcol));
          //            Serial.print("]");
            //digitalWrite(LEDPin[ledpin++], PatternBuf[patbufidx] & (1 << ledcol));
          }
          */
          //          Serial.print("]");
//          patbufidx++;
//        }
        //        Serial.println("]");
//        delay(500);
        // turn current plane on
        digitalWrite(PlanePin[plane], LOW);
        // delay PLANETIME us
        delayMicroseconds(PLANETIME);
      } // for plane
      Serial.println("]");
    } // while <EndTime
  } while (DisplayTime > 0); // read patterns until time=0 which signals end
}
